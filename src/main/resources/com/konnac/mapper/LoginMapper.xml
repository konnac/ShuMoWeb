<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.konnac.mapper.LoginMapper">
<!--验证账号密码-->
    <select id="getByUserNameAndPassword" resultType="com.konnac.pojo.User">
        select *
        from users
        where username = #{username}
        and password = #{password}
    </select>
</mapper>